{
  "name": "react-starter",
  "version": "1.2.0",
  "main": "index.js",
  "repository": "git@github.com:ink8bit/react-starter.git",
  "author": "ink8bit <ink8bit@users.noreply.github.com>",
  "license": "MIT",
  "engines": {
    "node": "11.9.0"
  },
  "private": true,
  "scripts": {
    "prestart": "npm run build",
    "start": "cd ./dist && npx serve",
    "dev": "npm audit && parcel src/index.html",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "build": "parcel build src/index.html --no-source-maps",
    "clean": "rm -rf ./dist",
    "lint": "eslint --fix \"**/*.{js,jsx}\"",
    "test": "jest",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --config  ./.prettierrc \"./src/*.{js,jsx,json,css,html}\" --write",
    "format:check": "prettier --list-different \"./src/*.{js,jsx,json,css,html}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "eslint --fix",
      "npm run lint",
      "git add",
      "npm t"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "eslint": "^5.14.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.2",
    "serve": "^10.1.2"
  },
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@reach/router": "^1.2.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-loadable": "^5.5.0"
  }
}
