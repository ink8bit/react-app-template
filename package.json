{
  "name": "react-starter",
  "version": "3.1.0",
  "main": "index.js",
  "repository": "git@github.com:ink8bit/react-starter.git",
  "author": "ink8bit <ink8bit@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prestart": "npm run build",
    "start": "NODE_ENV=production cd ./build && npx serve",
    "dev": "webpack-dev-server --port 3000 --config webpack.dev.js",
    "prebuild": "npm run lint && npm run test",
    "build": "npm run compile",
    "compile": "webpack --config webpack.prod.js",
    "lint": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint \"./src/**/*.css\"",
    "test": "jest --forceExit",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "test:ci": "npm t -- --config jest.config.ci.js",
    "prettier": "prettier \"./src/**/*.{js,jsx,css,html,json}\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --list-different"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "react-axe": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "serve": "^11.3.1",
    "style-loader": "^1.2.1",
    "stylelint": "^13.5.0",
    "stylelint-config-recommended": "^3.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
