{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "development": {
      "plugins": ["@babel/plugin-proposal-class-properties"]
    },
    "test": {
      "plugins": ["@babel/plugin-proposal-class-properties", "babel-plugin-dynamic-import-node"]
    }
  }
}
