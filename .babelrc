{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry"
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-class-properties", "emotion", "dynamic-import-node"],
  "env": {
    "development": {
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        ["emotion", { "sourceMap": true }],
        "dynamic-import-node"
      ]
    }
  }
}
